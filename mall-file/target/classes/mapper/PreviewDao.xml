<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC   
    "-//mybatis.org//DTD Mapper 3.0//EN"  
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mall.file.preview.dao.PreviewDao">
	<insert id="createFileSave">
		insert into file_document_info(id,host,fileName,suffix,path,createTime,status)values(#{id},#{host},#{fileName},#{suffix},#{path},#{createTime},0)
	</insert>


</mapper> 
    